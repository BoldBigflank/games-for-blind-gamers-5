{
  "name": "games-for-blind-gamers-5",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.4",
    "@vueuse/core": "^14.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "pinia": "^3.0.4",
    "prettier": "^3.8.1",
    "tsc-files": "^1.1.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vue": "^3.5.28"
  },
  "lint-staged": {
    "**/*.ts?(x)": "bash -c tsc --noEmit",
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": "prettier --write"
  },
  "dependencies": {},
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
